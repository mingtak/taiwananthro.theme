<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="taiwananthro.content">
<body>


<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">

<div id="core-block">

<h3>本期目錄</h3>
<hr>
    <tal:results define="batch context/getChildNodes">
      <tal:listing condition="batch">
        <div class="entries">
          <tal:repeat repeat="item batch">
<div class="article-item">
<a href="${item/absolute_url}"><h4 href="${item/absolute_url}"><b>${item/title}</b></h4></a>
<br>
<div tal:content="structure item/metaData/raw"></div>
<div style="display:none" class="article-abs">
 <div tal:replace="structure item/abstract/raw" />
</div>
<a href="javascript:void(0)" class="btn btn-info show-abs">閱讀摘要</a>
<hr>
</div>
          </tal:repeat>
        </div>

      </tal:listing>
    </tal:results>




</div>

</metal:content-core>
</metal:content-core>

</body>
</html>
